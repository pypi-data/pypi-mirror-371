# {{ project_name | title }} Environment Configuration

# Server Configuration (Pydantic format)
SERVER__HOST=localhost
SERVER__PORT=8002
SERVER__DEBUG=false

# Logging Configuration (Pydantic format)
LOGGING__LEVEL=INFO
LOGGING__FORMAT=structured
LOGGING__ENABLE_ACCESS_LOGS=true

# Connector Configuration (Clean naming pattern)
# Enable connectors by setting to 'true'
ENABLE_ENCODER=true

# Encoder Service Configuration (OAuth2 Example)
ENCODER_BASE_URL=https://api.example.com
ENCODER_TOKEN_URL=https://auth.example.com/oauth/token
ENCODER_CLIENT_ID=your_client_id
ENCODER_CLIENT_SECRET=your_client_secret
ENCODER_CODETYPE=cpt
ENCODER_CODE=99213

# Additional Connector Examples:
# You can add more connectors by creating JSON files in connectors/ directory
# and adding their environment variables here following the clean naming pattern:
#
